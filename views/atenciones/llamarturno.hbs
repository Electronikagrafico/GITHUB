        }

    }


    function Iniciar() {

        var Texto_btnIni = this.document.getElementById("EnAtencion_txt").innerHTML;

        switch (Texto_btnIni) {

            case "Iniciar Turno":
                pausar();
                document.getElementById("btn_Llamar_active").id = "btn_Llamar_disabled";
                document.getElementById("llamar_btn_llamando").id = "llamar_btn_disabled";
                document.getElementById("btn_Iniciar").id = "btn_Iniciar_active";
                document.getElementsByClassName("ausente")[0].className += " esconder";

                this.document.getElementById("EnAtencion_txt").innerHTML = "Finalizar turno";


                break;

            case "Finalizar turno":

                document.getElementById("btn_Llamar_disabled").id = "btn_Llamar_active";
                document.getElementById("llamar_btn_disabled").id = "llamar_btn_llamando";
                document.getElementById("btn_Iniciar_active").id = "btn_Iniciar";
                this.document.getElementById("EnAtencion_txt").innerHTML = "Iniciar Turno";


                break;


        }


    }

    function Esconder() {


        var Texto_btnIni = this.document.getElementById("EnAtencion_txt").innerHTML;

        const llamar_txt = document.querySelector("#llamar_txt");
        const btn_llamar = document.querySelector("#llamar_btn");

        switch (Texto_btnIni) {

            case "Iniciar Turno":

                // document.getElementById("llamar_btn_disabled").id = "btn_Llamar";
                this.document.getElementById("llamar_txt").innerHTML = "LLamar Turno";
                document.getElementById("panel_info_usuario_activo").id = "panel_info_usuario";
                document.getElementById("btn_Llamar_active").id = "btn_Llamar";
                document.getElementById("llamar_btn_llamando").id = "llamar_btn";
                document.getElementById("opciones_atencion_visible").id = "opciones_atencion";
                document.getElementById("opciones_atencion_visible").id = "opciones_atencion_2";
                document.getElementById("opciones_atencion_visible").id = "opciones_atencion_3";
                document.getElementById("opciones_atencion_visible").id = "opciones_atencion_4";

                break;

            case "Finalizar turno":

                //document.getElementById("llamar_btn_disabled").id = "btn_Llamar";
                this.document.getElementById("llamar_txt").innerHTML = "LLamar Turno";
                document.getElementById("panel_info_usuario_activo").id = "panel_info_usuario";
                document.getElementById("llamar_btn_disabled").id = "llamar_btn";
                document.getElementById("btn_Llamar_disabled").id = "btn_Llamar";
                document.getElementById("opciones_atencion_visible").id = "opciones_atencion";
                document.getElementById("opciones_atencion_visible").id = "opciones_atencion_2";
                document.getElementById("opciones_atencion_visible").id = "opciones_atencion_3";
                document.getElementById("opciones_atencion_visible").id = "opciones_atencion_4";
                document.getElementById("btn_Iniciar_active").id = "btn_Iniciar";
                this.document.getElementById("EnAtencion_txt").innerHTML = "Iniciar Turno";
                restablercer_tiempo();



