    function llamar() {


        const llamar_txt = document.querySelector("#llamar_txt");
        const btn_llamar = document.querySelector("#llamar_btn");

        if (llamar_txt.textContent == "LLamar Turno") {

            this.document.getElementById("llamar_txt").innerHTML = "Llamando...";
